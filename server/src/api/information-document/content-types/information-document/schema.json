{
  "kind": "collectionType",
  "collectionName": "information_documents",
  "info": {
    "singularName": "information-document",
    "pluralName": "information-documents",
    "displayName": "Information Document"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "required": true,
      "default": "PDF",
      "enum": [
        "PDF",
        "XLSX",
        "CSV",
        "DOCX"
      ]
    },
    "file": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "enumeration",
      "required": true,
      "default": "Rapport",
      "enum": [
        "Rapport",
        "Documentation",
        "Texte législatif ou réglementaire"
      ]
    },
    "information": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::information.information",
      "inversedBy": "information_documents"
    },
    "position": {
      "type": "enumeration",
      "required": true,
      "default": "list",
      "enum": [
        "main",
        "list"
      ]
    }
  }
}
