{
  "kind": "collectionType",
  "collectionName": "informations",
  "info": {
    "singularName": "information",
    "pluralName": "informations",
    "displayName": "Information"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "short": {
      "type": "string"
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "printables": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "files"
      ]
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "norder": {
      "type": "integer",
      "required": true,
      "unique": false
    },
    "publicationDate": {
      "type": "date"
    },
    "abstract": {
      "type": "text"
    },
    "parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::information.information",
      "inversedBy": "children"
    },
    "children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::information.information",
      "mappedBy": "parent"
    }
  }
}
