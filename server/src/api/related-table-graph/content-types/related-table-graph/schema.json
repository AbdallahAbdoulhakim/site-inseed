{
  "kind": "collectionType",
  "collectionName": "related_table_graphs",
  "info": {
    "singularName": "related-table-graph",
    "pluralName": "related-table-graphs",
    "displayName": "Related Table Graph"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "graphic": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::publication-graphic.publication-graphic"
    },
    "norder": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "publication": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::publication.publication",
      "inversedBy": "table_graphs"
    }
  }
}
