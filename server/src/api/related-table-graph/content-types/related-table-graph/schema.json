{
  "kind": "collectionType",
  "collectionName": "related_table_graphs",
  "info": {
    "singularName": "related-table-graph",
    "pluralName": "related-table-graphs",
    "displayName": "Related Table Graph"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "norder": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "publication": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::publication.publication",
      "inversedBy": "table_graphs"
    },
    "title": {
      "type": "string"
    },
    "inSummary": {
      "type": "boolean",
      "default": false
    },
    "link": {
      "type": "string",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "inSummary"
            },
            true
          ]
        }
      }
    },
    "datafile": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "files"
      ]
    }
  }
}
