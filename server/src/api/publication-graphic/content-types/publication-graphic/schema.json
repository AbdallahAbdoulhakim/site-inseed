{
  "kind": "collectionType",
  "collectionName": "publication_graphics",
  "info": {
    "singularName": "publication-graphic",
    "pluralName": "publication-graphics",
    "displayName": "Publication Graphic"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "dataurl": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "line",
        "scatter",
        "compound",
        "vertical barchart",
        "horizontal barchart",
        "compound stacked barchart"
      ]
    },
    "legend": {
      "type": "string"
    },
    "norder": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "datafile": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "files"
      ]
    },
    "publication": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::publication.publication",
      "inversedBy": "graphics"
    },
    "title": {
      "type": "string"
    },
    "yAxisLegend": {
      "type": "string"
    },
    "xAxisLegend": {
      "type": "string"
    },
    "subtitle": {
      "type": "string"
    },
    "inSummary": {
      "type": "boolean",
      "required": false,
      "default": false
    },
    "link": {
      "type": "string",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "inSummary"
            },
            true
          ]
        }
      }
    },
    "startFrom": {
      "type": "decimal",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "type"
            },
            "line"
          ]
        }
      }
    },
    "compoundLineKey": {
      "type": "string"
    }
  }
}
