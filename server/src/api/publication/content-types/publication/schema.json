{
  "kind": "collectionType",
  "collectionName": "publications",
  "info": {
    "singularName": "publication",
    "pluralName": "publications",
    "displayName": "Publication"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "parutionNumber": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "short": {
      "type": "string"
    },
    "abstract": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Informations Rapides",
        "Chiffres clés",
        "Série chronologiques",
        "Jeux de données",
        "Chiffres détaillés",
        "Bulletin IHPC",
        "Rapport d'analyse",
        "Comptes Nationaux"
      ]
    },
    "parutionDate": {
      "type": "datetime"
    },
    "parutionNext": {
      "type": "datetime"
    },
    "printable": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "files"
      ]
    },
    "data": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "files"
      ]
    },
    "publicationSlug": {
      "type": "string",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "external"
            },
            false
          ]
        }
      }
    },
    "publication_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::publication-category.publication-category",
      "inversedBy": "publications"
    },
    "publication_collections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::publication-collection.publication-collection",
      "inversedBy": "publications"
    },
    "publication_themes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::publication-theme.publication-theme",
      "inversedBy": "publications"
    },
    "publication_geos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::publication-geo.publication-geo",
      "inversedBy": "publications"
    },
    "paragraphs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::publication-paragraph.publication-paragraph",
      "mappedBy": "publication"
    },
    "graphics": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::publication-graphic.publication-graphic",
      "mappedBy": "publication"
    },
    "table_graphs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::related-table-graph.related-table-graph",
      "mappedBy": "publication"
    },
    "external": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "url": {
      "type": "string",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "external"
            },
            true
          ]
        }
      }
    },
    "hasSummary": {
      "type": "boolean",
      "default": true
    }
  }
}
