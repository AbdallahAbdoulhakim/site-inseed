{
  "kind": "collectionType",
  "collectionName": "publication_paragraphs",
  "info": {
    "singularName": "publication-paragraph",
    "pluralName": "publication-paragraphs",
    "displayName": "Publication Paragraph"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "link": {
      "type": "string",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "inSummary"
            },
            true
          ]
        }
      }
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultMarkdown"
      }
    },
    "norder": {
      "type": "integer",
      "required": true,
      "default": 1,
      "unique": false
    },
    "inSummary": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "publication": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::publication.publication",
      "inversedBy": "paragraphs"
    },
    "hasTableWithSpan": {
      "type": "boolean",
      "default": false
    }
  }
}
